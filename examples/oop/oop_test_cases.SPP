class Box {
    let width: int;
    let height: int;

    func init(w: int, h: int) {
        self.width = w;
        self.height = h;
    }

    func area(): int {
        return self.width * self.height;
    }

    func describe() {
        print("الصندوق: " + str(self.width) + "x" + str(self.height));
        print("المساحة: " + str(self.area()));
    }
}

func main() {
    let b: Box = Box(5, 3);
    b.describe();
}
