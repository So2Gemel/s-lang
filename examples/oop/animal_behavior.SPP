class Animal {
    let type: string;

    func init(t: string) {
        self.type = t;
    }

    func sound() {
        if self.type == "cat" {
            print("مواء");
        } else if self.type == "dog" {
            print("نبح");
        } else {
            print("صوت غير معروف");
        }
    }
}

func main() {
    let a: Animal = Animal("cat");
    a.sound();   // مواء
}
